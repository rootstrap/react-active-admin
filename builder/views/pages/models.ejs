<!DOCTYPE html>
<html lang="en">

    <head>
        <% include ../partials/head %>
    </head>

    <body class="container">
        <header>
            <% include ../partials/header %>
        </header>

        <main>
            <div class="jumbotron">
                <h1>Reax Builder</h1>
                <br>
                <h2>Models</h2>
                <ul>
                    <% models.forEach((model) => { %>
                        <li><%= model.name %></li>
                    <% }); %>
                </ul>
            </div>
        </main>

        <footer>
            <% include ../partials/footer %>
        </footer>
    </body>
</html>